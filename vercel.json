{
    "version": 2,
    "regions": [
        "gru1"
    ],
    "installCommand": ["yum makecache --refresh", "yum install -y yum-utils rpmdevtools", "yum install expat glib2 libffi libffi-dev cairo pango"],
    "builds": [
        {
            "src": "core/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb",
                "runtime": "python3.9"
            }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "headers": {
                "cache-control": "s-maxage=604800"
            },
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "core/wsgi.py"
        }
    ],
    "outputDirectory": "staticfiles"
}